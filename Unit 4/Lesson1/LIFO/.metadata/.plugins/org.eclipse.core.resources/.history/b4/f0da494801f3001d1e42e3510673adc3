#ifndef FIFO_H_
#define FIFO_H_

#include "Platform_Types.h"
#include <stdlib.h>

#define element_type usint8_t
#define NULL (void *)0
/*************User Data Type*************/
typedef struct {
	element_type length;
	element_type* head;
	element_type* base;
	element_type* tail;
	element_type count;

}FIFO_t;


#endif /* FIFO_H_ */
